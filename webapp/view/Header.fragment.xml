<c:FragmentDefinition xmlns:c="sap.ui.core" xmlns="sap.m" xmlns:l="sap.ui.layout" xmlns:html="http://www.w3.org/1999/xhtml">
    <l:VerticalLayout width="100%" id="masterTest">
        <l:content>
            <HBox alignContent="Center" alignItems="Center" id="toolBarHBox" justifyContent="SpaceBetween" width="100%" backgroundDesign="Transparent">
                <Button press="onNavigateHome" id="iconButton" class="sapUiTinyMarginBeginEnd" icon="sap-icon://home" text="Home" />
                <HBox id="rightSideItems" width="20%" justifyContent="End">
                    <Button press="navigateToAbout" id="about" text="Fahrrad bauen" />
                    <Button press="navigateToProfile" id="profile" text="Lagerverwaltung" />
                    <Button id="dropDownMenu" text="" icon="sap-icon://drop-down-list" press="navToTable" />
                </HBox>
            </HBox>
        </l:content>
    </l:VerticalLayout>
</c:FragmentDefinition>
